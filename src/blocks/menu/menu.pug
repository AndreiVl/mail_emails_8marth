			td.text.text--sm(align="center" style="padding: 0 10px; border-right: 1px solid #CCCCCC;")
				a.link.link--no-decor.link--text(href="#")!=data.link_1
			td.text.text--sm(align="center" style="padding: 0 10px;border-right: 1px solid #CCCCCC;")
				a.link.link--no-decor.link--text(href="#")!=data.link_2
			td.text.text--sm(align="center" style="padding: 0 10px;border-right: 1px solid #CCCCCC;")
				a.link.link--no-decor.link--text(href="#")!=data.link_3
			td.text.text--sm(align="center" style="padding: 0 10px;")
				a.link.link--no-decor.link--text(href="#")!=data.link_4