mixin store(data)
	table.table-important(align="left" border="0" cellpadding="0" cellspacing="0")
		tr 
			td(align="left" style="color: #666666;" width="94")
				+img({
					link: '#',
					img_link: 'img_store_apstore_94_28_01.jpg',
					alt: 'apstore',
					width: 94,
					height: 28
				})

			+td-width(14)

			td(align="left" style="color: #666666;" width="94")
				+img({
					link: '#',
					img_link: 'img_store_googleplay_94_28_01.jpg',
					alt: 'googleplay',
					width: 94,
					height: 28
				})